{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bugfix_translation",
      "camelcase": "BugfixTranslation",
      "title": "Bugfix Translation",
      "scope": "source.bugfix_translation",
      "file-types": [
        "bugfix_translation"
      ],
      "injection-regex": "^bugfix_translation$",
      "class-name": "TreeSitterBugfixTranslation"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Bugfix Translation",
    "authors": [
      {
        "name": "authors"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-bugfix_translation"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}