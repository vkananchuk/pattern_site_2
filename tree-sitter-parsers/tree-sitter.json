{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bugfix-translation",
      "camelcase": "BugfixTranslation",
      "path": "./bugfix-translation/",
      "title": "Bugfix Translation",
      "scope": "source.bugfix",
      "file-types": [
        "bugfix"
      ],
      "injection-regex": "^bugfix$",
      "class-name": "TreeSitterBugfixTranslation"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Pattern language for Bugfix data",
    "authors": [
      {
        "name": "-"
      }
    ],
    "links": {
      "repository": "https://example.com/"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
